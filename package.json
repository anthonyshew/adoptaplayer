{
  "name": "gatsby-starter-blog",
  "private": true,
  "description": "A starter for a blog powered by Gatsby, Markdown, and Netlify CMS",
  "version": "0.1.0",
  "author": "Anthony Shew",
  "scripts": {
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "run-p start:**",
    "start:app": "gatsby develop -p 3000",
    "start:lambda": "netlify-lambda serve src/.netlify/functions",
    "build": "gatsby build && netlify-lambda build src/.netlify/functions",
    "build:app": "gatsby build",
    "build:lambda": "netlify-lambda build src/.netlify/functions",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.5.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gatsby": "^2.19.32",
    "gatsby-image": "^2.2.43",
    "gatsby-plugin-feed": "^2.3.28",
    "gatsby-plugin-google-analytics": "^2.1.37",
    "gatsby-plugin-manifest": "^2.2.44",
    "gatsby-plugin-netlify-cms": "^4.1.41",
    "gatsby-plugin-offline": "^3.0.37",
    "gatsby-plugin-react-helmet": "^3.1.23",
    "gatsby-plugin-sharp": "^2.4.7",
    "gatsby-remark-copy-linked-files": "^2.1.38",
    "gatsby-remark-images": "^3.1.46",
    "gatsby-remark-prismjs": "^3.3.33",
    "gatsby-remark-responsive-iframe": "^2.2.33",
    "gatsby-remark-smartypants": "^2.1.22",
    "gatsby-source-filesystem": "^2.1.50",
    "gatsby-transformer-remark": "^2.6.55",
    "gatsby-transformer-sharp": "^2.3.17",
    "netlify-cms-app": "^2.11.27",
    "prismjs": "^1.19.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "http-proxy-middleware": "^1.0.2",
    "netlify-lambda": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "serverless-http": "^2.3.2"
  }
}